{
	"extends": [
		"next/core-web-vitals",
		"next/typescript",
		"eslint:recommended",
		"plugin:import/recommended"
	],
	"plugins": ["import"],
	"rules": {
		"import/order": [
			"error",
			{
				"groups": [
					["builtin", "external"],
					["internal"],
					["sibling", "parent"],
					["index"]
				],
				"alphabetize": {
					"order": "asc",
					"caseInsensitive": true
				},
				"newlines-between": "always"
			}
		]
	}
}
